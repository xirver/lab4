{
  "target": "bmv2",
  "p4info": "build/streaming.p4.p4info.txt",
  "bmv2_json": "build/streaming.json",
  "table_entries": [
    {
      "table": "MyIngress.ipv4_lpm",
      "default_action": true,
      "action_name": "MyIngress.drop",
      "action_params": { }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dstAddr": ["10.0.3.3", 32]
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
        "dstAddr": "08:00:00:00:03:33",
        "port": 1
      }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dstAddr": ["10.0.7.7", 32]
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
        "dstAddr": "08:00:00:00:05:00",
        "port": 4
      }
    },  
    {
      "table": "MyIngress._multi",
      "match": {
        "hdr.ipv4.dstAddr": ["10.0.7.7", 32],
        "hdr.ipv4.srcAddr": "10.0.1.1"
      },
      "action_name": "MyIngress.multicast",
      "action_params": {
      }
    },  
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4.dstAddr": ["10.0.1.1", 32]
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
        "dstAddr": "08:00:00:00:02:00",
        "port": 3
      }
    }
  ],
  "multicast_group_entries": [{
    "multicast_group_id": 1,
    "replicas": [{
    "egress_port": 1,
    "instance": 1
    }]
  }]
}